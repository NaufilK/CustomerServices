		<MultiInput showClearIcon="true" enabled="{=${appView>/process} !== 'EXTEND' ? true : false}" value="{Customers>/zgc_proposed_inco_term}" maxLength="3" valueHelpRequest="handleValueHelpForIncoterms"
	 valueHelpOnly="true" visible="{=${fieldMappingModels>/IncoTermVisible} === true ? true : false}"/>

	
	</forms:SimpleForm>
</mvc:View>